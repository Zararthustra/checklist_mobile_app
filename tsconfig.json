{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "@assets/*": ["./src/assets/*"],
      "@interfaces/*": ["./src/interfaces/*"],
      "@queries/*": ["./src/queries/*"],
      "@forms/*": ["./src/forms/*"],
      "@utils/*": ["./src/utils/*"],
      "@components/*": ["./src/components/*"],
      "root/*": ["./*"]
    }
  }
}
